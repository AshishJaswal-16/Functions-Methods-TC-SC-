import java.util.*;

public class FirstClass{                //Print name
    public static void myName(String name){
        System.out.println(name);
        return;

    }
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        String name = sc.next();
        myName(name);
    }



    public static int calculateSum(int a, int b){   //Sum of 2 no's
       int sum =a + b;
       return sum;
    }
    public static void main(String args[]){
        Scanner sc =new Scanner(System.in);
        int a =sc.nextInt();
        int b = sc.nextInt();

        int sum= calculateSum(a,b);
        System.out.println("Sum of the two no's is :"+sum);

    }





    public static void factorialNo(int n){          //Factorial No
        if(n<0){
            System.out.println("Invalid number");
            return;
        }
        int factorial = 1;
        for(int i=n;i>=1;i--){
            factorial=factorial*i;
        }
        System.out.println(factorial);
    }
    public static void main(String args[]){
        Scanner sc =new Scanner(System.in);
        int n = sc.nextInt();
        factorialNo(n);
    }
}
